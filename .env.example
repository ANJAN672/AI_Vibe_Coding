# Supabase Configuration (Required for memory features)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI API Keys (Configure at least one)
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here

# Default LLM Provider (openai, gemini, claude, mistral)
DEFAULT_LLM_PROVIDER=openai

# n8n Integration (Optional - can be configured in UI)
# N8N_HOST_URL=http://localhost:5678
# N8N_API_KEY=your_n8n_api_key_here

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000